function task31(){
    let isDrunk = true
    let age = 19

    if( isDrunk === false && age >= 18){
    console.log('Możesz się napić piwka w barze!')
    } else if ( isDrunk === true && age >= 18){
    console.log('Jesteś pijany i nie sprzedamy Ci alkoholu')
    } else if ( isDrunk === true && age < 18){
    console.log('Jakim cudem ? Musisz wytrzeźwieć zanim wrócisz do domu!')
    }else {
    console.log('Jesteś za młody!!')
}
}

function task32(){
let isDrunk = false
let age = 19

isDrunk === false && age >= 18 ? console.log('Możesz się napić piwka w barze!') 
: isDrunk === true && age >= 18 ? console.log('Jesteś pijany i nie sprzedamy Ci alkoholu') 
: isDrunk === true && age < 18 ? console.log('Jakim cudem ? Musisz wytrzeźwieć zanim wrócisz do domu!') 
: console.log('Jesteś za młody!!')
}

function task33(){
    console.log( true&&false) // false
    console.log( true||false) // true
    console.log( false||true) // true
    console.log( true&&true) // true
}

function task34(){
    const i =-1
    let number =''

    i>0 ? number='dodatnia' : number='ujemna'
    console.log(number,i)
}

